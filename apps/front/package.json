{
	"name": "front",
	"version": "1.0.0",
	"description": "Project of Me",
	"type": "module",
	"scripts": {
		"dev": "next dev --port 3100 --hostname local.myapp.com --experimental-https",
		"dev:mock": "concurrently \"pnpm mock\" \"next dev --port 3100\"",
		"dev:inspect": "cross-env NODE_OPTIONS='$NODE_OPTIONS --inspect' next dev",
		"build": "next build",
		"start": "next start --port 3100",
		"lint": "next lint",
		"format": "prettier --write ./src",
		"test": "jest",
		"mock": "tsx watch ./src/mocks/http.ts",
		"mock:init": "msw init ./public",
		"test:watch": "jest --watchAll",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"extract-schema": "pnpm run delete:schema && pnpm openapi-generate",
		"openapi-generate": "tsx ./src/schema/openAPIGenerator.ts",
		"delete:schema": "npx rimraf ./src/schema/auth ./src/schema/non-auth",
		"preinstall": "npx only-allow pnpm"
	},
